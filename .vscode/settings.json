{
    "workbench.startupEditor": "newUntitledFile",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.executorMap": {
        "javascript": "node",
        "python": "python",
        "perl": "perl",
        "go": "go run",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc \"$fileName\" -lm -O2 -Wall -pedantic -o \"./exe/$fileNameWithoutExt\" && \"./exe/$fileNameWithoutExt\" ",
        "cpp": "cd $dir && g++ \"$fileName\" -O2 -Wall -pedantic -o \"./exe/$fileNameWithoutExt\" && \"./exe/$fileNameWithoutExt\" "
    },
    "explorer.confirmDragAndDrop": false,
    "C_Cpp.updateChannel": "Insiders",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.errors.incompleteClasspath.severity": "ignore",
    "terminal.integrated.inheritEnv": true,
    "python.jediEnabled": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Peacock Light (rainglow)",
    "terminal.external.linuxExec": "konsole",
    "terminal.integrated.automationShell.linux": "",
    "git.enableSmartCommit": true,
    "emmet.syntaxProfiles": { "javascript": "jsx" },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
}